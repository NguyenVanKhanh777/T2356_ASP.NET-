@model IEnumerable<DevXuongMoc.Models.AdminLog>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Areas/Admins/Views/Shared/_LayoutAdmin.cshtml";
}

<h1 class="text-center my-4">Danh sách Admin Logs</h1>

<p>
    <a class="btn btn-success" asp-action="Create">Tạo Mới</a>
</p>

<table class="table table-bordered table-striped">
    <thead class="thead-dark">
        <tr>
            <th>@Html.DisplayNameFor(model => model.Status)</th>
            @* <th>@Html.DisplayNameFor(model => model.SessionId)</th>
            <th>@Html.DisplayNameFor(model => model.AppCode)</th>
            <th>@Html.DisplayNameFor(model => model.ThreadId)</th>
            <th>@Html.DisplayNameFor(model => model.StartTime)</th>
            <th>@Html.DisplayNameFor(model => model.EndTime)</th>
            <th>@Html.DisplayNameFor(model => model.AdminUserId)</th> *@
            <th>@Html.DisplayNameFor(model => model.UserLogin)</th>
            <th>@Html.DisplayNameFor(model => model.UserName)</th>
            @* <th>@Html.DisplayNameFor(model => model.IpAddress)</th> *@
            @* <th>@Html.DisplayNameFor(model => model.ActionCode)</th> *@
            @* <th>@Html.DisplayNameFor(model => model.ActionName)</th> *@
            @* <th>@Html.DisplayNameFor(model => model.MenuCode)</th> *@
            @* <th>@Html.DisplayNameFor(model => model.MenuName)</th> *@
            @* <th>@Html.DisplayNameFor(model => model.WebpagesActionId)</th> *@
            @* <th>@Html.DisplayNameFor(model => model.ActionType)</th> *@
            @* <th>@Html.DisplayNameFor(model => model.ObjectId)</th> *@
            <th>@Html.DisplayNameFor(model => model.Content)</th>
            <th>@Html.DisplayNameFor(model => model.Description)</th>
            @* <th>@Html.DisplayNameFor(model => model.LogLevel)</th> *@
            @* <th>@Html.DisplayNameFor(model => model.ErrorCode)</th> *@
            <th>ACTION</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@Html.DisplayFor(modelItem => item.Status)</td>
                @* <td>@Html.DisplayFor(modelItem => item.SessionId)</td>
                <td>@Html.DisplayFor(modelItem => item.AppCode)</td>
                <td>@Html.DisplayFor(modelItem => item.ThreadId)</td>
                <td>@Html.DisplayFor(modelItem => item.StartTime)</td>
                <td>@Html.DisplayFor(modelItem => item.EndTime)</td>
                <td>@Html.DisplayFor(modelItem => item.AdminUserId)</td> *@
                <td>@Html.DisplayFor(modelItem => item.UserLogin)</td>
                <td>@Html.DisplayFor(modelItem => item.UserName)</td>
                @* <td>@Html.DisplayFor(modelItem => item.IpAddress)</td>
                <td>@Html.DisplayFor(modelItem => item.ActionCode)</td>
                <td>@Html.DisplayFor(modelItem => item.ActionName)</td>
                <td>@Html.DisplayFor(modelItem => item.MenuCode)</td>
                <td>@Html.DisplayFor(modelItem => item.MenuName)</td>
                <td>@Html.DisplayFor(modelItem => item.WebpagesActionId)</td>
                <td>@Html.DisplayFor(modelItem => item.ActionType)</td>
                <td>@Html.DisplayFor(modelItem => item.ObjectId)</td> *@
                <td>@Html.DisplayFor(modelItem => item.Content)</td>
                <td>@Html.DisplayFor(modelItem => item.Description)</td>
                @* <td>@Html.DisplayFor(modelItem => item.LogLevel)</td> *@
                @* <td>@Html.DisplayFor(modelItem => item.ErrorCode)</td> *@
                <td>
                    <a class="btn btn-warning btn-sm" asp-action="Edit" asp-route-id="@item.LogId">Sửa</a>
                    <a class="btn btn-info btn-sm" asp-action="Details" asp-route-id="@item.LogId">Chi Tiết</a>
                    <a class="btn btn-danger btn-sm" asp-action="Delete" asp-route-id="@item.LogId">Xóa</a>
                </td>
            </tr>
        }
    </tbody>
</table>
